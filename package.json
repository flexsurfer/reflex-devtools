{
  "name": "reflex-devtools-workspace",
  "version": "0.1.0",
  "description": "Developer tools for applications built with the reflex library",
  "private": true,
  "packageManager": "pnpm@10.13.1",

  "scripts": {
    "build": "pnpm -r build",
    "build:devtools": "pnpm --filter @flexsurfer/reflex-devtools build",
    "build:ui": "pnpm --filter reflex-devtool-ui build",
    "dev": "pnpm -r dev",
    "dev:ui": "pnpm --filter reflex-devtool-ui dev",
    "dev:server": "pnpm --filter @flexsurfer/reflex-devtools start",
    "dev:testapp": "pnpm --filter reflex-test-app dev"
  },
  "keywords": ["devtools", "debugging", "react", "react-native", "reflex"],
  "author": "flexsurfer",
  "license": "MIT"
}