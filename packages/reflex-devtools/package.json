{
  "name": "@flexsurfer/reflex-devtools",
  "version": "0.1.2",
  "description": "Developer tools for applications built with the reflex library",
  "author": "flexsurfer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flexsurfer/reflex.git"
  },
  "type": "module",
  "main": "dist/client/index.js",
  "types": "dist/client/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "bin": {
    "reflex-devtools": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.12.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ws": "^8.5.4",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@flexsurfer/reflex": ">=0.1.12"
  },
  "keywords": [
    "devtools",
    "debugging",
    "react",
    "react-native",
    "reflex"
  ]
}
